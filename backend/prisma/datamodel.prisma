type User {
    id: ID! @id
    firstName: String!
    lastName: String!
    email: String! @unique
    password: String!
    images: [Image!]!
    createdAt: DateTime! @createdAt
    updatedAt: DateTime! @updatedAt
}

type Image {
    id: ID! @id
    postedBy: User
    url: String!
    createdAt: DateTime! @createdAt
    updatedAt: DateTime! @updatedAt
}